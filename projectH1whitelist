local whitelist = {
    "FilteringDisabledEz",
    "gameeboy335",
    "aHmoDEAC2",
    "1337probacon",
    "1337service",
    "H1iaIt",
}
valid = false
wait(0.5)

for i,white in pairs(whitelist) do
    wait()
    if white == game.Players.LocalPlayer.Name then
        valid = true
    end
end
wait(5)
if valid == false then
    local k = Instance.new("Message",game.Workspace)
	k.Text = "you are not whitelisted, crashing in 5 secs..."
	wait(3)
	while true do
		print("CRASH")
		for i = 1, 2000 do
			Instance.new("Part",game.workspace)
		end
	end
end

if valid == true then
  print("Whitelisted!")
end
